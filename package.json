  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tony-xlh/opencvjs-document-scanner/issues"
  },
  "homepage": "https://github.com/tony-xlh/opencvjs-document-scanner#readme",
  "keywords": [
    "document",
    "opencv",
    "scanning",
    "correction",
    "detection",
    "dynamsoft"
